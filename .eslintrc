{
  "rules": {
    "quotes": [
      2,
      "single",
      "avoidEscape": true,
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-dangle": [
      2,
      "always-multiline"
    ]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "slate": true
  },
  "extends": "eslint:recommended"
}
